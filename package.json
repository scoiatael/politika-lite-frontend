{
  "name": "react-boilerplate-lite",
  "version": "0.0.0",
  "description": "Minimal boilerplate for React apps",
  "scripts": {
    "start": "node server/server.js",
    "start:dashboard": "webpack-dashboard node server/server.js",
    "postinstall": "npm run build",
    "build": "webpack --config webpack/webpack.prod.config.js",
    "clean": "rm -rf build",
    "lint": "tslint {src,test}/**/*.{ts,tsx}",
    "test": "jest"
  },
  "author": "Michał Załęcki <michal@michalzalecki.com>",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "^.+\\.spec\\.(js|ts)x?$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "dependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.1",
    "compression": "^1.7.3",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.3.6",
    "style-loader": "^0.23.0",
    "typescript": "^3.0.3",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.23.1"
  }
}
